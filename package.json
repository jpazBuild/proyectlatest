{
  "name": "hellolatest",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open --env allure=true --browser chrome",
    "cy:run": "cypress run --config video=true --env allure=true --browser chrome",
    "allure:report": "allure generate allure-results --clean -o allure-report",
    "allure:clear": "rm -rf allure-results/* allure-report/latest cypress/screenshots || true",
    "allure:history": "mv -f allure-report/latest/history allure-results/history && rm -r allure-report/latest || true"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "allure-commandline": "^2.20.1",
    "cypress": "^12.2.0"
  },
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.34.0"
  }
}
